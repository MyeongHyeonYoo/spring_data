package polymorphism;

public class LgTV {
	@Override
	public void turnOn() {
		System.out.println("LgTV---전원 켠다");
	}
	@Override
	public void turnOff( ) {
		System.out.println("LgTV---전원 끊다");
	}
	@Override
	public void soundUp() {
		System.out.println("LgTV---소리 올린다");
	}
	@Override
	public void soundDown() {
		System.out.println("LgTV---소리 내린다.");
	}
}
